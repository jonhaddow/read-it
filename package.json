{
	"name": "read-it",
	"version": "1.0.0",
	"description": "Store links from across the web",
	"main": "dist/client/index.ts",
	"engines": {
		"node": ">14.x"
	},
	"scripts": {
		"dev:client": "webpack serve",
		"dev:server": "nodemon ./src/server/index.ts",
		"dev": "concurrently \"npm:dev:client\" \"npm:dev:server\" -c green,blue",
		"build:client": "cross-env NODE_ENV=production webpack --env=production",
		"build:server": "tsc -p src/server",
		"start:server": "node dist/server/index.js",
		"test": "jest",
		"typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
		"typeorm:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -o -n ",
		"typeorm:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
		"typeorm:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jonhaddow/read-it.git"
	},
	"author": {
		"email": "haddowjon@protonmail.com",
		"name": "Jon Haddow",
		"url": "https://jon.haddow.me"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jonhaddow/read-it/issues"
	},
	"homepage": "https://github.com/jonhaddow/read-it#readme",
